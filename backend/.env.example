# Backend .env.example
# Copie este arquivo para .env.local (localhost) ou use .env.production (EasyPanel)

# Supabase (obtenha em https://supabase.com)
SUPABASE_URL=https://sua-projeto-id.supabase.co
SUPABASE_ANON_KEY=sua_anon_key_aqui
SUPABASE_SERVICE_KEY=sua_service_role_key_aqui

# JWT Secret (mesmo do Supabase -> Settings -> API -> JWT Secret)
JWT_SECRET=seu_jwt_secret_aqui

# API Configuration
API_HOST=0.0.0.0
API_PORT=8001  # localhost: 8001, easypanel: 3333
API_DEBUG=true  # localhost: true, production: false

# CORS (ajuste conforme seus dom√≠nios)
ALLOWED_ORIGINS=["http://localhost:3000"]

# AI/ML Config
YOLO_MODEL=yolo11n.pt
YOLO_CONFIDENCE=0.6

# Bridge Authentication (defina uma chave segura)
BRIDGE_API_KEY=sua_bridge_api_key_aqui

# Logging
LOG_LEVEL=INFO  # localhost: DEBUG, production: INFO