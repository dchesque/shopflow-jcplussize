[camera]
# Câmera Intelbras Mibo RTSP URLs
rtsp_url = rtsp://[IP-CAMERA]:554/cam/realmonitor?channel=1&subtype=0
rtsp_fallback = rtsp://[IP-CAMERA]:554/cam/realmonitor?channel=1&subtype=1
username = [USUARIO-CAMERA]
password = [SENHA-CAMERA]

[server]
# Desenvolvimento local (descomente para testar)
