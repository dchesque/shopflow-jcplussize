{
  "camera": {
    "rtsp_url": "rtsp://admin:password@192.168.1.100:554/stream1",
    "fallback_url": "rtsp://admin:password@192.168.1.100:554/stream2",
    "fps": 25,
    "resolution": {
      "width": 1920,
      "height": 1080
    },
    "reconnect_interval": 5,
    "max_retries": 10,
    "timeout": 30
  },
  "server": {
    "vps_url": "http://localhost:8001",
    "api_key": "bridge_api_key_123",
    "upload_endpoint": "/api/bridge/frames",
    "heartbeat_endpoint": "/api/bridge/heartbeat",
    "heartbeat_interval": 30,
    "max_frame_size": 1048576,
    "compression_quality": 85
  },
  "streaming": {
    "hls_enabled": true,
    "hls_segment_duration": 2,
    "hls_playlist_size": 5,
    "output_dir": "./hls_output",
    "mjpeg_enabled": true,
    "mjpeg_port": 8080,
    "mjpeg_quality": 80
  },
  "processing": {
    "pre_detection": true,
    "motion_threshold": 1000,
    "send_empty_frames": false,
    "frame_skip": 2,
    "roi": {
      "enabled": false,
      "x": 0,
      "y": 0,
      "width": 100,
      "height": 100
    }
  },
  "logging": {
    "level": "INFO",
    "file": "logs/bridge.log",
    "max_size": "10MB",
    "backup_count": 5,
    "console": true
  },
  "monitoring": {
    "prometheus_enabled": false,
    "prometheus_port": 8090,
    "health_check_port": 8091
  }
}